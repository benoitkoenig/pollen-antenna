{
  "name": "@pollen-antenna/data-storage",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "prestart": "rimraf dist && tsc --incremental && docker compose up -d",
    "start": "pnpm run --parallel start:func start:build",
    "start:func": "dotenvx run -f .env.local -- func start",
    "start:build": "tsc -w --incremental --preserveWatchOutput",
    "populate-subdivisions": "dotenvx run -f .env.local -- node --experimental-strip-types scripts/populate-subdivisions/index.ts"
  },
  "dependencies": {
    "@apollo/server": "^4.10.4",
    "@azure/functions": "^4.0.0",
    "google-auth-library": "^10.5.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.12.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.3",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@dotenvx/dotenvx": "^0.44.5",
    "@pollen-antenna/static-data": "workspace:^",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.12.12",
    "rimraf": "^5.0.0",
    "typescript": "^5.9.2"
  },
  "main": "dist/src/functions/*/index.js"
}
